{
   "env": {
        "browser": true,
        "es2021": true
    },
    "extends": "standard-with-typescript",
    "parserOptions": {
        "ecmaVersion": "latest",
        "sourceType": "module"
    },
    "rules": {
      "dot-notation": "off",
      "typescript-eslint/no-floating-promises":"off"
    },
    "ignorePatterns": [
      "migrations"
    ]
}
